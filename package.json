{
  "name": "template-preact-ssr",
  "version": "0.0.1",
  "description": "",
  "main": "public/index.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client:dev:start": "webpack-dev-server --progress --config ./webpack.client.dev.js --watch",
    "client:dev:compile": "webpack --config ./webpack.client.dev.js",
    "client:prod:start": "webpack-dev-server --progress --config ./webpack.client.prod.js --watch",
    "client:prod:compile": "webpack --config ./webpack.client.prod.js",
    "cds": "npm run client:dev:start",
    "cdc": "npm run client:dev:compile",
    "cdcw": "npm run cdc -- -- --watch",
    "cps": "npm run client:prod:start",
    "cpc": "npm run client:prod:compile",
    "cpcw": "npm run cpc -- -- --watch",
    "renderer:start": "nodemon --exec babel-node src/server.js",
    "renderer:compile": "COMPILE=true webpack --config ./webpack.renderer.js",
    "sitemap:start": "nodemon --exec babel-node src/sitemap/server.js",
    "sitemap:compile": "COMPILE=true webpack --config ./webpack.sitemap.js",
    "rs": "npm run renderer:start",
    "rc": "npm run renderer:compile",
    "ss": "npm run sitemap:start",
    "sc": "npm run sitemap:compile",
    "build": "npm run cpc && npm run rc && npm run sc",
    "now-build": "npm run cpc",
    "deploy": "git add . && git commit -m '[Auto] Compile and deploy from cli' && git push",
    "build:deploy": "npm run build && npm run deploy",
    "bd": "npm run build:deploy",
    "test-eslint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwrlines/template-preact-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fwrlines/template-preact-ssr/issues"
  },
  "homepage": "https://github.com/fwrlines/template-preact-ssr#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@fwrlines/alphabet-scss": "^0.9.0",
    "@loadable/component": "^5.11.0",
    "@loadable/server": "^5.11.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-retry": "^2.2.15",
    "date-fns": "^2.8.1",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0",
    "preact": "^10.1.1",
    "preact-render-to-string": "^5.1.3",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@loadable/babel-plugin": "^5.11.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-preact": "^2.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-synacor": "^3.0.5",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "postcss": "^7.0.26",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-loadable-ssr-addon": "^0.2.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
